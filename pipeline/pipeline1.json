{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "S3 to Blob",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"AIRPORT_ID": "AIRPORT_ID",
							"AIRPORT": "AIRPORT",
							"DISPLAY_AIRPORT_NAME": "DISPLAY_AIRPORT_NAME",
							"LATITUDE": "LATITUDE",
							"LONGITUDE": "LONGITUDE"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "S3",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob1",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Notebook1",
				"type": "DatabricksNotebook",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"notebookPath": "/dataengineering with databricks/1 Datafactory ETL"
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"inputPath": {
				"type": "string",
				"defaultValue": "/staged_sink"
			},
			"outputPath": {
				"type": "string",
				"defaultValue": "/processed_sink"
			},
			"filename": {
				"type": "string",
				"defaultValue": "Product.csv"
			}
		}
	}
}